/*
problem
: printout times table(multiplication table) according to input N
condition
: 1<=N<=9
Example
-Input
2
-Output
2 * 1 = 2
2 * 2 = 4
2 * 3 = 6
2 * 4 = 8
2 * 5 = 10
2 * 6 = 12
2 * 7 = 14
2 * 8 = 16
2 * 9 = 18
*/

#include<iostream>

using namespace std;

int main(){
    int N;
    cin>>N;
    for(int i=1;i<=9;i++){
        cout<<N<<" * "<<i<<" = "<<N*i<<endl;
    }
}
